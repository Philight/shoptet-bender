      "../assets/shared/js/libs/tracking.js",
      "../assets/shared/js/libs/image360.js",
      "../assets/shared/js/libs/stockAvailabilities.js",
      "../assets/shared/js/libs/variants/surcharges.js",
      "../assets/shared/js/libs/variants/common.js",
