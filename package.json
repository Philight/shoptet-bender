    "dest": "dist/build.js"
  },
  "uglifyJS": {
    "options": {
      "mangle": false
    },
    "files": {
      "dist/build.min.js": [
        "dist/build.js"
      ]
    }
  },
  "dependencies": {
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "babel-loader": "^9.1.3",
    "browser-sync": "^2.27.10",
