    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.8.0",
    "path": "^0.12.7",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.1",
    "webpack-obfuscator": "^3.5.1",
    "webpack-remove-empty-scripts": "^1.0.4"
